                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>&amp;lt;div attr1=&amp;quot;attr1 value&amp;quot; attrwithtrue array-attr=&amp;quot;1,2,3&amp;quot;&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; Attrs added via map&lt;/p>&#xa;&lt;p>&amp;lt;/div&amp;gt;&lt;/p>&#xa;"
