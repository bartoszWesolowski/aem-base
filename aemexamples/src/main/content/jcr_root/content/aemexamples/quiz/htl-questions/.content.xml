                <textquestion_316337568
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemexamples/components/quiz/question/textQuestion">
                    <question
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>What are the values of v1-v4 ? Will the div element get displayed in the followign snippet ?&lt;/p>&#xa;&lt;p style=&quot;margin-left: 40px;&quot;>&amp;lt;div data-sly-test.v1=&amp;quot;${'value-1'}&amp;quot; data-sly-test.v4=&amp;quot;${true}&amp;quot; data-sly-test.v2=&amp;quot;${''}&amp;quot; data-sly-test.v3=&amp;quot;${'new value'}&amp;quot; &amp;gt;&lt;/p>&#xa;&lt;p style=&quot;margin-left: 40px;&quot;>&amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;lt;p&amp;gt;Div content in sly test&amp;lt;/p&amp;gt;&lt;/p>&#xa;&lt;p style=&quot;margin-left: 40px;&quot;>&amp;lt;/div&amp;gt;&lt;/p>&#xa;"
                        textIsRich="true"/>
                    <answer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>1. The div element will not be displayed&lt;/p>&#xa;&lt;p>v1 - value-1&lt;/p>&#xa;&lt;p>v4 - true&lt;/p>&#xa;&lt;p>v2 - ''&lt;/p>&#xa;&lt;p>v3 - ''&lt;/p>&#xa;&lt;p>&lt;b>Test variables after the first one that has evaluated to false will not be evaluated!&amp;nbsp;&lt;/b>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true"/>
                </textquestion_316337568>
