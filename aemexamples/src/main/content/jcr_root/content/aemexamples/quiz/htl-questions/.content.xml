                </textquestion_1974233620>
                <textquestion_374494956
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemexamples/components/quiz/question/textQuestion">
                    <question
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>Assuming that foobar = {'id' : 'foo', 'class' : 'bar'}&lt;/p>&#xa;&lt;p>what will be the value of:&lt;/p>&#xa;&lt;p>&amp;lt;input data-sly-attribute=&amp;quot;${foobar}&amp;quot; type=&amp;quot;text&amp;quot;/&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true"/>
