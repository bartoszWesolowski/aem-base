                <textquestion_2072303165
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemexamples/components/quiz/question/textQuestion">
                    <question
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>Given:&lt;/p>&#xa;&lt;p style=&quot;margin-left: 40px;&quot;>foobar = {'id': 'foo', 'class': 'bar', 'lang': ''}&lt;/p>&#xa;&lt;p>What will be the value of:&amp;nbsp;&lt;/p>&#xa;&lt;ol>&#xa;&lt;li>&amp;lt;div class=&amp;quot;bar1&amp;quot; data-sly-attribute.class=&amp;quot;bar2&amp;quot; data-sly-attribute=&amp;quot;${foobar}&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;li>&amp;lt;div data-sly-attribute=&amp;quot;${foobar}&amp;quot; data-sly-attribute.class=&amp;quot;bar2&amp;quot; id=&amp;quot;foo2&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;/ol>&#xa;"
                        textIsRich="true"/>
                    <answer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;ol>&#xa;&lt;li>&amp;lt;div id=&amp;quot;foo&amp;quot; class=&amp;quot;bar&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;li>&amp;lt;div id=&amp;quot;foo2&amp;quot; class=&amp;quot;bar2&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;/ol>&#xa;&lt;p>&lt;b>Arguments are processed from left to right - element on the left takes precedence ofver the one on the left !&lt;/b>&lt;/p>&#xa;"
                        textIsRich="true"/>
                </textquestion_2072303165>
                <textquestion_1265104619
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemexamples/components/quiz/question/textQuestion">
                    <answer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>No - all will render empty div.&amp;nbsp;&lt;/p>&#xa;&lt;p>Empty attribiutes &lt;b>will be removed&lt;/b>. This does not apply for normal attribuets added with literal value passed:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>class=&amp;quot;&amp;quot; - will be rendered&lt;/li>&#xa;&lt;li>class=&amp;quot;${''}&amp;quot; - will not be rendered&lt;/li>&#xa;&lt;li>data-sly-attribiute=&amp;quot;&amp;quot; - will not be rendered&lt;/li>&#xa;&lt;li>data-sly-attribiute=&amp;quot;${''}&amp;quot; - will not be rendered&lt;/li>&#xa;&lt;/ul>&#xa;"
                        textIsRich="true"/>
                    <question
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>Will any of the following div render the lang atribiute?&lt;/p>&#xa;&lt;p style=&quot;margin-left: 40px;&quot;>&amp;lt;div lang=&amp;quot;${''}&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;br>&#xa;&amp;lt;div lang=&amp;quot;en&amp;quot; data-sly-attribute.lang&amp;gt;&amp;lt;/div&amp;gt;&lt;br>&#xa;&amp;lt;div lang=&amp;quot;en&amp;quot; data-sly-attribute.lang=&amp;quot;&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;br>&#xa;&amp;lt;div lang=&amp;quot;en&amp;quot; data-sly-attribute.lang=&amp;quot;${''}&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/p>&#xa;"
                        textIsRich="true"/>
                </textquestion_1265104619>
                <textquestion_949988179
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="aemexamples/components/quiz/question/textQuestion">
                    <question
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;p>What will be rendered for:&lt;/p>&#xa;&lt;ol>&#xa;&lt;li>&amp;lt;div title=&amp;quot;${['one', 'two', 'three']}&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;li>&amp;lt;div title=&amp;quot;${[]}&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;/ol>&#xa;"
                        textIsRich="true"/>
                    <answer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/text"
                        text="&lt;ol>&#xa;&lt;li>&amp;lt;div title=&amp;quot;one,two,three&amp;quot;&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;li>&amp;lt;div&amp;gt;&amp;lt;/div&amp;gt;&lt;/li>&#xa;&lt;/ol>&#xa;&lt;p>Arrays are converted to Strings. Empty array is interpreted as empty value and will remove the attribuyte form HTML code.&lt;/p>&#xa;"
                        textIsRich="true"/>
                </textquestion_949988179>
                <textquestion_1574173388
